{
  "name": "go-study-utfpr-server",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "dev": "tsnd --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "lint:eslint": "eslint . --ext .js,.ts --fix",
    "lint:tsc": "tsc --noEmit --esModuleInterop",
    "lint": "run-p lint:eslint lint:tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "tsc --noEmit --esModuleInterop",
      "eslint . --ext .js,.ts --fix"
    ]
  },
  "repository": "https://github.com/FlavioBernaski/GoStudy-UTFPR-server",
  "author": "Eh os TI",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3",
    "mysql2": "^2.3.3",
    "sequelize": "^6.18.0",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "1.3.1",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.2"
  },
  "packageManager": "yarn@3.2.0"
}
